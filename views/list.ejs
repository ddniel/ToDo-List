<%- include("header") -%>   <!-- Usa toda la informacion del archivo header.ejs-->

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    <div class="box" >
        <% for (let i=0; i<newListItems.length; i++){ %>
          <div class="item">
            <input type="checkbox"> <!--Solo crea el checkbox-->
            <p><%= newListItems[i] %></p>
          </div>
        <% } %>

        <form class="item" action="/" method="post">
          <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
          <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>

    </div>


<%- include("footer") -%> <!-- Usa toda la informacion del archivo footer.ejs-->
